<div class="branding">
  <div class="container-fluid">
    <div class="wrap">
      <h1 class="logo">
        <a href="<%- url_for() %>">
          <% if (config.logo){ %>
            <img src="<%- url_for(config.logo)%>" alt="<%= config.title %>">
          <% } else { %>
            <%= config.title %>
          <% } %>
        </a>
      </h1>
      <% if (config.subtitle){ %>
        <h2 class="subtitle"><%= config.subtitle %></h2>
      <% } %>
      <%- partial('_partial/navigation', null, {cache: !config.relative_link}) %>
    </div>
  </div>
</div>
