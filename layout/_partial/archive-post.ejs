<article class="archive-post post-type-<%= post.layout %>">
  <div class="archive-post-inner">
    <header class="archive-post-header">
      <%- partial('post/date', {class_name: 'archive-post-date', date_format: 'MMM D'}) %>
      <%- partial('post/title', {class_name: 'archive-post-title', index: true}) %>
    </header>
  </div>
</article>