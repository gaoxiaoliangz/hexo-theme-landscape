{
  "name": "whitepaper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar source/_src/style/ -o source/css/ --source-map-contents",
    "watch-css": "npm run build-css && node-sass-chokidar source/_src/style/ -o source/css/ --watch --recursive --source-map"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-es2015": "^6.24.1",
    "grunt": "~0.4.2",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-git": "~0.2.2",
    "grunt-webpack": "^3.0.2",
    "load-grunt-tasks": "~0.2.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "@gxl/webpack-utils": "^3.0.0",
    "node-sass-chokidar": "^0.0.3"
  }
}
